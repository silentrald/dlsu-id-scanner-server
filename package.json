{
  "name": "id-scanner-server",
  "version": "1.0.0",
  "main": "src/app.ts",
  "author": "silentrald",
  "private": true,
  "scripts": {
    "dev": "tsnd --respawn -r tsconfig-paths/register src/app.ts",
    "test": "yarn test:unit",
    "test:unit": "jest --coverage=true",
    "db-up": "ts-node -r tsconfig-paths/register src/migrations/core/cmd.ts up",
    "db-down": "ts-node -r tsconfig-paths/register src/migrations/core/cmd.ts down",
    "db-ins": "ts-node -r tsconfig-paths/register src/migrations/core/cmd.ts ins",
    "db-del": "ts-node -r tsconfig-paths/register src/migrations/core/cmd.ts del",
    "db-reset": "yarn db-down && yarn db-up && yarn db-ins"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "ioredis": "^5.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^28.1.8",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.5",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "jest": "^29.0.1",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  }
}
